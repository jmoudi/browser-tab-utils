{
  "name": "tabs-utils",

  "permissions": [
    "file:///*",
    "<all_urls>",
    "activeTab",
    "browserSettings",    
    "bookmarks",
    "browsingData",
    "contextMenus",
    "downloads",
    "history",
    "notifications",
    "nativeMessaging",
    "sessions",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "theme",
    "contextualIdentities"
  ],
  "background": {
    "scripts": [
       "background.js"
    ]
  },
  "commands": {
    "close-duplicate-tabs": {
            "suggested_key": { "default": "Alt+Shift+C" },
            "description": "duplicate"
    }
  },


  "manifest_version": 2,
  "version": "1.0.0",
  "applications": { "gecko": { "id": "duplicate-tabs@jmoudi" }}
}
