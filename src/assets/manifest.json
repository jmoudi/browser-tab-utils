{
  "name": "{{name}}",

  "permissions": [
    "<all_urls>",
    "activeTab",
    "browserSettings",    
    "bookmarks",
    "browsingData",
    "contextMenus",
    "downloads",
    "history",
    "menus",
    "menus.overrideContext",
    "notifications",
    "nativeMessaging",
    "sessions",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "theme",
    "contextualIdentities"
  ],
  "background": {
    "scripts": [
       "background.js"
    ]
  },
  "commands": {
    "close-duplicate-tabs": {
      "suggested_key": { 
        "default": "Alt+Shift+C" 
      },
          "description": "duplicate"
    }
  },


  "manifest_version": 2,
  "version": "{{version}}",
  "applications": { "gecko": { "id": "{{name}}@jmoudi" }}
}
